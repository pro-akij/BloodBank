<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.something.iccworldcup2019">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/wlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true"/>

        <activity android:name=".LiveScore"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".PointTable"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Team_Activety.Team_AFG"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_WIN"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_SR"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_BAN"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_PK"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_AU"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_RSA"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_NZ"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_IN"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team_Activety.Team_EN"
            android:parentActivityName=".Squads"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Squads"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Venues"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Team"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Schedule"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>